<%= render TurboModalComponent.new(title: "Employments") do %>
  <div id="employments">
    <%= render partial: "employments/employment", collection: @employments,
           as: :employment, locals: {editable: false} %>
  </div>
  <%= turbo_frame_tag "employments" do%>
    <div class="p-6 lg:p-8 text-center">
      <%= link_to "New employment", new_personal_datum_employment_path(@personal_datum), class: "w-full text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"  %>
    </div>
  <%end%>
<%end%>
